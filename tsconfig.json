{
    "compilerOptions": {
      "target": "ES5",
      "module": "ES2015",
      "lib": [
        "ESNext",
        "DOM"
      ],
      "moduleResolution": "Node",
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      // "noEmit": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "noImplicitReturns": true,
      "declaration": true,
      "emitDeclarationOnly": true,
      "outDir": "dist",
      "noImplicitAny": false
    },
    "include": [
      "src/*.ts",
      "src/*.d.ts",
      "index.d.ts"
    ]
  }